<launch>
  <arg name="is_cartographer" default="false"/>
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf" output="screen">
    <rosparam command="load" file="$(find turn_on_wheeltec_robot)/config/ekf.yaml"/>
    <param name="two_d_mode" value="true"/>
  </node>
</launch>

